{
  "name": "rmit-fintech-club-web",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "license": "",
  "author": "",
  "scripts": {
    "bundle-firebase-script": "watchify -v -t babelify js/firebase/script.js -p esmify -o js/bundles/firebase/script-bundle.js",
    "bundle-login": "watchify -v -t babelify js/login.js -p esmify -o js/bundles/login-bundle.js",
    "build": "parcel build index.html",
    "ssg:blog": "node posts/app.js",
    "ssg:projects": "node projects/app.js",
    "ssg": "npm run ssg:blog && npm run ssg:projects",
    "dev": "parcel index.html --open",
    "start": "npm run build && npm run ssg && npm run dev",
    "format": "prettier --write **/*.{js,json,md,html,scss,css} !{assets,node_modules,.cache}/** !js/bundles/** !bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "prettier": "2.4.1",
    "sass": "^1.43.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "babelify": "^10.0.0",
    "calendar-link": "^2.1.1",
    "dotenv": "^10.0.0",
    "esmify": "^2.1.1",
    "firebase": "^9.6.0",
    "firebase-admin": "^10.0.1",
    "marked": "^4.0.8",
    "uuid": "^8.3.2",
    "watchify": "^4.0.0"
  }
}
